{
    "manifest_version": 3,
    "name": "Wanted Memory",
    "version": "1.0.0",
    "description": "Chrome extension that tracks and marks visited job postings on Wanted.",
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.wanted.co.kr/*"],
        "css": ["css/content.css"],
        "js": ["js/content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
}